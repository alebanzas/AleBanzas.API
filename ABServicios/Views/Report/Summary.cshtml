@model Dictionary<string, int>
<h2></h2>

<table style="float: left;">
    @{
        var i = 0;
        foreach (var data in Model.OrderBy(x => x.Key))
        {
            i += data.Value;
            <tr>
                <td>@data.Key</td>
                <td>@data.Value</td>
            </tr>
        }
    }
    <tr>
        <td>TOTAL</td>
        <td>@i</td>
    </tr>
</table>

<table style="float: left;margin-left: 20px;">
    @{
        foreach (var data in Model.OrderByDescending(x => x.Value))
        {
            <tr>
                <td>@data.Key</td>
                <td>@data.Value</td>
            </tr>
        }
    }
    <tr>
        <td>TOTAL</td>
        <td>@i</td>
    </tr>
</table>

<div style="clear: both;"></div>