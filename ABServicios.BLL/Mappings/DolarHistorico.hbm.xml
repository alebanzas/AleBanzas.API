<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
           assembly="ABServicios.BLL"
				   namespace="ABServicios.BLL.Entities">
  <class name="DolarHistorico" table="DOLAR_Historico">
    <id name="ID">
      <generator class="guid"/>
    </id>

    <property name="Date" column="date" />
    <property name="Compra" column="valorCompra" />
    <property name="Venta" column="valorVenta" />
    <property name="Moneda" column="tipoMoneda" />
    
  </class>
</hibernate-mapping>